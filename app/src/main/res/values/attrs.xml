<resources>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference"/>
        <attr name="metaButtonBarButtonStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="PasswordValidator">
        <attr name="underlineErrorColor" format="color"/>
        <attr name="underlineSuccessColor" format="color"/>
        <attr name="textError" format="string"/>
    </declare-styleable>

    <declare-styleable name="SummaryText">
        <attr name="st_image" format="integer"/>
        <attr name="st_title" format="string"/>
        <attr name="st_summary" format="string"/>
    </declare-styleable>

    <declare-styleable name="ItemMain">
        <attr name="im_image" format="integer"/>
        <attr name="im_et" format="string"/>
    </declare-styleable>

    <declare-styleable name="ItemMainSpinner">
        <attr name="ims_icon" format="integer"/>
        <attr name="ims_editspin" format="string"/>
    </declare-styleable>

</resources>
